<?xml version="1.0" encoding="utf-8" ?>
<configuration >
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
<!--
日志输出级别
    error: 错误,系统故障日志
    warn: 警告,存在风险或者使用不当
    info: 一般性信息
    debug: 程序内部用于调试
    trace: 程序内部用于追终   
-->
    <root level="debug">
        <appender-ref ref="console"/>
    </root>
</configuration>